/network add -autosendcmd "/^msg nickserv ident MYPASS;wait 2000" freenode
/server add -auto -ssl -network freenode irc.freenode.net
/channel add -auto #archlinuxvn freenode
/channel add -auto #theslinux freenode
/set term_charset utf-8
/log open -targets #theslinux ~/logs/theslinux/%Y/%m/theslinux-%Y%m%d.log
/log open -targets #archlinuxvn ~/logs/archlinuxvn/%Y/%m/archlinuxvn-%Y%m%d.log
